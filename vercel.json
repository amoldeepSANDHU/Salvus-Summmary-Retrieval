{
    "version": 2,
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      }
    ],
   
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "install": {
      "include": ["package.json", "package-lock.json"]
    },
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
  